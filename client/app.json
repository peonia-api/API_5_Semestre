{

  "expo": {
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(cadastro_equipamentos) to access your camera."
        }
      ]
    ],
    "name": "cadastro_equipamentos",
    "slug": "cadastro_equipamentos",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.cadastro_equipamentos.cadastro_equipamentos"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
